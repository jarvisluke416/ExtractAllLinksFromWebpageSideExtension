{
    "manifest_version": 3,
    "name": "AllLinks",
    "version": "1.0",
    "description": "Extension with a default side panel.",
    "icons": {
      "16": "icon.png"
    },
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "permissions": ["sidePanel", "tabs", "scripting"],
    "host_permissions": ["<all_urls>"]
  }
  