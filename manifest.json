{
  "manifest_version": 3,
  "name": "Link Wrapper",
  "version": "1.0",
  "description": "Extension with a default side panel.",
  "icons": {
  "99": "icon.png"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "permissions": ["sidePanel","tabs",
    "scripting",
    "activeTab"],
    "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}